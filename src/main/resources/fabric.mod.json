{
	"schemaVersion": 1,
	"id": "lodestone",
	"version": "${version}",
	"name": "Lodestone",
	"description": "Library for various tasks including creating VFX effects in Minecraft. Ported from Lodestone by SammySemicolon.",
	"authors": [
		"Bawnorton",
		"SammySemicolon"
	],
	"contact": {
		"homepage": "https://github.com/Bawnorton/Lodestone",
		"sources": "https://github.com/Bawnorton/Lodestone"
	},
	"license": "GPLv3",
	"icon": "assets/lodestone/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"team.lodestar.lodestone.Lodestone"
		],
		"client": [
			"team.lodestar.lodestone.client.LodestoneClient"
		],
		"fabric-datagen": [
			"team.lodestar.lodestone.data.LodestoneDataGenerator"
		]
	},
	"mixins": [
		"lodestone.mixins.json",
		{
			"config": "lodestone.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "lodestone.accesswidener",
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}